@{
	ViewBag.Title = "Error";
}

<div class="text-center">
	<h1>Error</h1>
	<h2 class="text-red">
		@if (ViewBag.ErrorMessage != null)
		{
			@ViewBag.ErrorMessage
		}
		else
		{
			<span>Error occured during execution!</span>
		}
	</h2>
	<img src="~/error.svg" width="500px" style="margin-top:100px" />
</div>
